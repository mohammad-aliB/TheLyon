    }
    #navbar ul{
    display: table;
    line-height: 50px;
    margin: 0 auto;
    padding: 0;
    }
    #navbar ul li{
    display: inline-block;
    height: 100%;
    width: 50px;
    /*text-align: center;*/
    margin: 0 10px;

    }
