    }
    #navbar-mobile{
    font-size: 100px;
    width: 10px;
     display: none; 
    /*margin: auto;*/
    width: 100%;
    height: auto;
    /*line-height: 50px;*/
    text-decoration: none;
    text-align: center;
    background: white;
}

    #navbar-ul li:first-child{
        display:none;
    }

    #calendar{
        width:760px;
        height:auto;
        /*border:1px solid black;*/
        margin:100px auto 0 auto;

    }
    #day{
        box-sizing: border-box;
    -moz-box-sizing: border-box;
    -webkit-box-sizing: border-box;
