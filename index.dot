    /* display: block; */
    }
    #schedule{
    box-sizing: border-box;
    -moz-box-sizing: border-box;
    -webkit-box-sizing: border-box;
        height:200px;
        width:50%;
        float:right;
        position: relative;
        border-right:1px solid black;
        border-top:1px solid black;
        border-bottom:1px solid black;
        /*background-color: blue;*/
    }
    #schedule ul{
    list-style: none;
    padding: 0;
    margin: 0;
    }
    #schedule ul .date{
        background-color:grey;
        height:auto;
        padding-left:10px;
    }
    #schedule ul .event{
        /*border-bottom:1px solid black;*/
        height:auto;
    }

    #schedule ul .event a{
        text-decoration: none;
        padding-left:10px;
        color:black;
        display:block;
        height:100%;
    }
    #schedule ul .event a:hover{
        border-left:1px solid red;
        /*background-color: grey;*/
    }
    #schedule #view-calander{
        text-decoration: none;
        width:100%;
        height:auto;
        bottom:0;
        position: absolute;
        text-align: center;
    }
    #schedule #view-calander:hover{
    background-color: grey;
    }
    #articles{
        max-width: 960px;
        margin: 0 auto 0 auto;
        padding-top:200px;
        margin-bottom: 100px;
    /* padding-top: 900px; */
    /*background-color: black;*/

        list-style: none;
    }
    #articles li{
        padding-top:20px;
    }
    #articles a{
        text-decoration: none;
    }
    #articles .title{
        display: inline-block;
        color:blue;
        /*border-bottom:1px solid black;*/
        font-size: 150%;
        width:100%;
        margin-bottom: -5px;
    }
    #articles .date-author{
        list-style: none;
        padding-left:0;
        padding-top:5px;
    }
    #articles .date{
        display: inline;
        padding-right:20px;
    }
    #articles .author{
        display:inline;
    }
    .article{
        border-bottom: 1px solid black;
        padding-bottom: 10px;
    }
    #footer{
        bottom:0px;
        width:100%;
        height:50px;
        background-color:black;
        margin: 0;
        padding: 0; 
    }
    @media only screen and (max-device-width:680px) (orientation : landscape), (orientation : portrait)   {
            #calendar{
                /*height:400px;*/
                width:90%;
                font-size:200%;

            }
            #day{
                float:none;
                width:100%;
            }
            #schedule{
                height:auto;
                float:none;
                width:100%;
                position: inherit;
                border-left:1px solid black;
        z-index: -1;/* to prevent overlap with mobile navbar*/
            }
            #schedule #view-calander{
                position: inherit;
                display:block;
            }
            #navbar{
                height:150px;
            }
        #navbar ul{
            display:block;
            position:fixed;
            width:100%;
            height:100%;
            top:-100%;
            background-color: white;

            transition-property: all;
            transition-duration: .5s;
        }
        #navbar ul li{
            margin:0;
            width:100%;
            height:10%;
        }
        #navbar ul li a{
            margin:0;
        }
        #navbar ul li a span{
            margin: auto;
            position: absolute;
            top: 50%;
            left: 50%;
            transform: translate(-50%, -50%);
            font-size:300%;
        }

        #navbar-ul.open{
            top:0;
        }
       #navbar-mobile{
            /*top:0;*/
    display: block;

        }
        #navbar-ul li:first-child{
            display:inherit;
        }
        #articles{
            font-size:200%;
        }
    }
</style>
</head>

